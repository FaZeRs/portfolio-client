{
  "name": "portfolio-client",
  "type": "module",
  "version": "1.7.0",
  "private": true,
  "packageManager": "pnpm@7.25.1",
  "description": "Portfolio website based on Vite and UnoCSS",
  "author": "Nauris Linde <naurislinde@gmail.com> (https://github.com/FaZeRs)",
  "license": "MIT",
  "homepage": "https://naurislinde.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FaZeRs/portfolio-client.git"
  },
  "keywords": [
    "portfolio",
    "vite",
    "ssg",
    "unocss"
  ],
  "engines": {
    "node": "19.x"
  },
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "test:e2e": "cypress open",
    "cypress:install": "cypress install",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.2",
    "@headlessui/vue": "^1.7.10",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.0.26",
    "nprogress": "^0.2.0",
    "vee-validate": "^4.7.4",
    "vue": "^3.2.47",
    "vue-demi": "^0.13.11",
    "vue-easy-lightbox": "^1.12.0",
    "vue-router": "^4.1.6",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify-json/carbon": "^1.1.15",
    "@sentry/vite-plugin": "^0.4.0",
    "@types/node": "^18.14.0",
    "@types/nprogress": "^0.2.0",
    "@unocss/preset-attributify": "^0.50.0",
    "@unocss/preset-icons": "^0.50.0",
    "@unocss/preset-typography": "^0.50.0",
    "@unocss/preset-web-fonts": "^0.50.0",
    "@unocss/reset": "^0.50.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue-macros/volar": "^0.8.3",
    "@vue/test-utils": "^2.3.0",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.6.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "pnpm": "^7.27.1",
    "taze": "^0.8.5",
    "typescript": "^4.9.5",
    "unocss": "^0.50.0",
    "unplugin-auto-import": "^0.14.4",
    "unplugin-vue-components": "^0.24.0",
    "unplugin-vue-macros": "^1.8.3",
    "vite": "^4.1.4",
    "vite-plugin-inspect": "^0.7.15",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-pwa": "^0.14.4",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-ssg": "^0.22.1",
    "vite-ssg-sitemap": "^0.4.3",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.1.7"
  }
}
