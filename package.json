{
  "name": "portfolio-client",
  "type": "module",
  "version": "1.1.1",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "description": "Portfolio website based on Vite and UnoCSS",
  "author": "Nauris Linde <naurislinde@gmail.com> (https://github.com/FaZeRs)",
  "license": "MIT",
  "homepage": "https://naurislinde.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FaZeRs/portfolio-client.git"
  },
  "keywords": [
    "portfolio",
    "vite",
    "ssg",
    "unocss"
  ],
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "cypress:install": "cypress install",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@vueuse/core": "^9.2.0",
    "@vueuse/head": "^0.7.11",
    "nprogress": "^0.2.0",
    "vee-validate": "^4.6.7",
    "vue": "^3.2.39",
    "vue-demi": "^0.13.11",
    "vue-easy-lightbox": "^1.8.2",
    "vue-final-modal": "^3.4.4",
    "vue-router": "^4.1.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@iconify-json/carbon": "^1.1.7",
    "@types/nprogress": "^0.2.0",
    "@unocss/preset-attributify": "^0.45.21",
    "@unocss/preset-icons": "^0.45.21",
    "@unocss/preset-typography": "^0.45.21",
    "@unocss/preset-web-fonts": "^0.45.21",
    "@unocss/reset": "^0.45.21",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/test-utils": "^2.0.2",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^10.8.0",
    "eslint": "^8.23.1",
    "eslint-plugin-cypress": "^2.12.1",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.11.0",
    "taze": "^0.8.0",
    "typescript": "^4.8.3",
    "unocss": "^0.45.21",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.0",
    "vite-plugin-inspect": "^0.7.1",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-pwa": "^0.12.7",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-ssg": "^0.20.2",
    "vite-ssg-sitemap": "^0.4.1",
    "vitest": "^0.23.2",
    "vue-tsc": "^0.40.13"
  }
}
